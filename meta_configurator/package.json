{
  "name": "config-assistant",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p build-only",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:ts": "eslint --ext .jsx,.js,.ts,.tsx --ignore-path .eslintignore",
    "lint:ls": "ls-lint",
    "lint": "npm run lint:ts && npm run lint:ls",
    "prettier": "prettier --write src/ --ignore-path .eslintignore",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "preview": "vite preview",
    "prepare": "cd ../.. && husky install .husky",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@jsonhero/schema-infer": "^0.1.5",
    "@vue-flow/core": "^1.33.6",
    "@vueuse/core": "^10.6.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "brace": "^0.11.1",
    "csv-parse": "^5.5.6",
    "dagrejs": "^0.2.1",
    "flowbite": "^1.6.5",
    "js-yaml": "^4.1.0",
    "json-cst": "^1.2.0",
    "json-pointer": "^0.6.2",
    "json-schema-faker": "^0.5.3",
    "json-schema-merge-allof": "^0.8.1",
    "json-schema-resolver": "^2.0.0",
    "json-schema-to-typescript": "^13.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "pinia": "^2.0.36",
    "primeicons": "^7.0.0",
    "primevue": "^3.29.2",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.1",
    "@types/json-pointer": "^1.0.31",
    "@types/node": "^18.16.8",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "editorconfig-checker": "5.0.1",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "json-schema-to-ts": "^2.8.2",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.1.4",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
